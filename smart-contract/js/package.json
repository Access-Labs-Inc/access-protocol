{
  "name": "@accessprotocol/js",
  "version": "2.1.1",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --coverage",
    "dev": "tsc && node dist/test.js",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && yarn build:esm && yarn build:cjs && ./fixup.sh",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "yarn build",
    "lint": "yarn pretty && eslint .",
    "lint:fix": "yarn pretty:fix && eslint . --fix",
    "pretty": "prettier --check 'src/*.[jt]s'",
    "pretty:fix": "prettier --write 'src/*.[jt]s'",
    "doc": "yarn typedoc src/index.ts",
    "amman:start": "amman start",
    "amman:stop": "amman stop"
  },
  "files": [
    "dist/esm/*",
    "dist/cjs/*",
    "dist/esm/types/*",
    "dist/cjs/types/*",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@metaplex-foundation/amman": "^0.12.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/bn.js": "^5.1.1",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.14.20",
    "@types/tmp": "^0.2.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^27.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "renamer": "^4.0.0",
    "save-dev": "0.0.1-security",
    "tmp": "^0.2.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.22.3",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "^1.66.2",
    "bn.js": "^5.2.0",
    "borsh": "^0.7.0",
    "bs58": "4.0.1"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  }
}
